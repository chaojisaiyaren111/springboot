<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springboot.cache.model.mapper.DepartmentMapper">
    <select id="entityById" resultType="com.springboot.cache.model.entity.Department" parameterType="java.lang.Integer">
        select * from department where id = #{id}
    </select>
</mapper>